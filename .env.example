# Qilbee Mycelial Network - Environment Variables Template
# Copy this file to .env and fill in your actual values

# API Keys for AI Providers (for testing live agents)
# Get your keys from:
# - https://console.anthropic.com/
# - https://platform.openai.com/
ANTHROPIC_API_KEY=your_api_key_here
OPENAI_API_KEY=your_api_key_here

# Database Configuration
POSTGRES_HOST=localhost
POSTGRES_PORT=5432
POSTGRES_DB=qmn
POSTGRES_USER=qmn_user
POSTGRES_PASSWORD=change_this_password

# Redis Configuration
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=

# MongoDB Configuration (for Hyphal Memory)
MONGODB_URI=mongodb://localhost:27017/qmn_memory

# Service URLs (Development)
QMN_IDENTITY_URL=http://localhost:8001
QMN_KEYS_URL=http://localhost:8002
QMN_ROUTER_URL=http://localhost:8003
QMN_MEMORY_URL=http://localhost:8004
QMN_POLICIES_URL=http://localhost:8005

# QMN Configuration
QMN_TENANT_ID=dev-tenant
QMN_ENVIRONMENT=development
QMN_LOG_LEVEL=INFO

# Security
SECRET_KEY=generate_a_random_secret_key_here
JWT_SECRET=generate_another_random_secret_here

# Feature Flags
ENABLE_REINFORCEMENT_LEARNING=true
ENABLE_GOSSIP_PROTOCOL=true
ENABLE_METRICS=true
